<div >
    <div class="row">
        <div class="col-md-3">
            <div class="contact-info">
                <img src="https://cdn3.iconfinder.com/data/icons/street-food-and-food-trucker-1/64/noodles-shop-food-store-street-512.png" alt="image" width="50" height="50"/>
                <h2>Registre su ContenedorFileSystemProducto</h2>
            </div>
        </div>
        <div class="col-md-9">
            <div class="contact-form">
                <form onsubmit="return addProduct(this)">
                    <div class="form-group">
                        <label class="control-label col-sm-2" >Nombre:</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="title" id="title" placeholder="Ingrese Nombre del ContenedorFileSystemProducto" >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">Precio:</label>
                        <div class="col-sm-2">
                            <input type="text" class="form-control" name="price" id="price" placeholder="Ingrese el Precio" >
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">URL Foto:</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="thumbnail" id="thumbnail" placeholder="Ingrese URL de la imagen del producto" >
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button type="submit" class="btn btn-default">Enviar</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>